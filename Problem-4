package Collection;

public class Problem4 
{
	public static void main(String[] args) 
	{
		int a[]={1,2,3,4,5,6,7,8,9};
		int input[]={1,2,8,9,12,46,76,82,15,20,30};
		int count =0;
		for(int i=0;i<=a.length-1;i++)
		{
			int m=a[i];
			count=0;
			for(int j=0;j<=input.length-1;j++)
			{
				
				if(input[j]%m==0)
				{
					count++;
				}
				
			}
			System.out.print(a[i]+" :"+count+" ,");
		}
		
	}

}
